@model Operation
@using Toplivo.Web.Models.Toplivo;
@{
    ViewBag.Title = "Подробно Операции";

}
<h2>Просмотр данных операции</h2>
<div>
    <hr />

    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Fuel.FuelType)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Fuel.FuelType)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Tank.TankType)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Tank.TankType)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Inc_Exp)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Inc_Exp)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Date)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Date)
        </dd>
    </dl>
</div>
<p>
    @Html.ActionLink("Возврат", "Details", new { id = -1 }) |
    @Html.ActionLink("Редактировать", "Edit", new { id = Model.OperationID })
</p>

